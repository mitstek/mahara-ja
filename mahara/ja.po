msgid "Trusted sites"
msgstr "信頼されたサイト"

#: lang/ja.utf8/admin.php updatesiteoptions
msgctxt "lang/ja.utf8/admin.php updatesiteoptions"
msgid "Update site options"
msgstr "サイトオプションを更新する"

